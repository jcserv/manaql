{
  "name": "manaql",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@9.7.1",
  "scripts": {
    "build": "rm -rf dist && tsc --project tsconfig.build.json && tsc-alias -p tsconfig.build.json && pnpm sentry:sourcemaps",
    "start": "node dist/src/index.js",
    "husky": "husky",
    "lint": "eslint .",
    "lint:fix": "prettier . --write && eslint . --fix",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:types": "tsc --noEmit",
    "dev": "pnpm prisma:gen && ts-node-dev -r tsconfig-paths/register --transpile-only src/index.ts",
    "dev:db": "docker compose -p manaql -f docker-compose.yml up --detach",
    "dev:db:down": "docker compose -p manaql -f docker-compose.yml down -v",
    "test:db": "docker compose -p manaql-test -f docker-compose.test.yml up --detach",
    "test:db:down": "docker compose -p manaql-test -f docker-compose.test.yml down -v",
    "prisma:pull": "prisma db pull",
    "gen": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "clean": "rm -rf dist node_modules",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org jarrod-servilla --project manaql ./dist && sentry-cli sourcemaps upload --org jarrod-servilla --project manaql ./dist"
  },
  "dependencies": {
    "@apollo/server": "^4.11.2",
    "@pothos/core": "^4.3.0",
    "@pothos/plugin-prisma": "^4.3.1",
    "@pothos/plugin-relay": "^4.3.0",
    "@prisma/adapter-pg": "^5.22.0",
    "@prisma/client": "5.22.0",
    "@sentry/cli": "^2.39.0",
    "@sentry/node": "^8.40.0",
    "@sentry/profiling-node": "^8.40.0",
    "dotenv": "^16.4.5",
    "graphql": "^16.9.0",
    "graphql-scalars": "^1.23.0",
    "graphql-tag": "^2.12.6",
    "pg": "^8.13.1",
    "tsc-alias": "^1.8.10"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@faker-js/faker": "^9.2.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.0",
    "eslint": "^9.15.0",
    "globals": "^15.12.0",
    "graphql-request": "^7.1.2",
    "husky": "^9.1.7",
    "prettier": "^3.3.3",
    "prisma": "^5.22.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.15.0",
    "vitest": "^2.1.5"
  }
}